<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Amasty_Mostviewed/js/ampack" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeIntegrations/js/amasty/mostviewed/ampack</item>
                            </item>
                            <item name="Amasty_Mostviewed/js/ampackcart" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeIntegrations/js/amasty/mostviewed/ampackcart</item>
                            </item>
                            <item name="Amasty_Mostviewed/js/ampackcart_popup" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeIntegrations/js/amasty/mostviewed/ampackcart_popup</item>
                            </item>
                            <item name="Amasty_Mostviewed/js/swatch-renderer" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeIntegrations/js/amasty/mostviewed/swatch-renderer</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
